{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFManagementPipeline"
		},
		"DS_AzureDataLake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DS_AzureDataLake'"
		},
		"DS_SqlServer1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DS_SqlServer1'",
			"defaultValue": "Integrated Security=False;Data Source=MURALIYARLI;Initial Catalog=EMPLOYEE;User ID=mky"
		},
		"DS_AzureDataLake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://blobstored.dfs.core.windows.net/"
		},
		"sqlkey_AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://newkeysforadf.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFManagementPipeline_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DS_AzureDataLake_accountKey": {
						"value": "[parameters('DS_AzureDataLake_accountKey')]"
					},
					"DS_SqlServer1_connectionString": {
						"value": "[parameters('DS_SqlServer1_connectionString')]"
					},
					"DS_AzureDataLake_properties_typeProperties_url": {
						"value": "[parameters('DS_AzureDataLake_properties_typeProperties_url')]"
					},
					"sqlkey_AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('sqlkey_AzureKeyVault1_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}